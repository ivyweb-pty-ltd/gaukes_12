<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record model='ir.module.category' id='category_financial_management'>
    <field name="name">Financial Services</field>
    <field name="description">User access levels for financial Services</field>
    <field name="sequence">3</field>
</record>

    <record model="res.groups" id="crm_attooh.financial_services_user">
        <field name="name">User</field>
        <field name="implied_ids" eval="[(4, ref('base.user_root'))]"/>
        <field name="category_id" ref="crm_attooh.category_financial_management"/>
    </record>

    <record model="res.groups" id="crm_attooh.financial_services_manager">
        <field name="name">Manager</field>
        <field name="implied_ids" eval="[(4,ref('financial_services_user'))]"/>
        <field name="category_id" ref="crm_attooh.category_financial_management"/>
    </record>
</odoo>